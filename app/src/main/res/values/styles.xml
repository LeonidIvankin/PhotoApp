<resources>

	<!-- Base application theme. -->
	<style name="AppTheme" parent="Theme.AppCompat.Light.DarkActionBar">
		<!-- Customize your theme here. -->
		<item name="colorPrimary">@color/colorPrimary</item>
		<item name="colorPrimaryDark">@color/colorPrimaryDark</item>
		<item name="colorAccent">@color/colorAccent</item>
	</style>

	<style name="AppThemeColorsBlueOrange" parent="Theme.AppCompat.Light.DarkActionBar">
		<item name="colorPrimary">@color/theme_blue_orange_primary</item>
		<item name="colorPrimaryDark">@color/theme_blue_orange_primary_dark</item>
		<item name="colorAccent">@color/theme_blue_orange_accent</item>
	</style>

	<style name="AppThemeColorsDeepPurpleGreen" parent="Theme.AppCompat.Light.DarkActionBar">
		<item name="colorPrimary">@color/colors_deep_purple_green_primary</item>
		<item name="colorPrimaryDark">@color/colors_deep_purple_green_primary_dark</item>
		<item name="colorAccent">@color/colors_deep_purple_green_accent</item>
	</style>

</resources>
